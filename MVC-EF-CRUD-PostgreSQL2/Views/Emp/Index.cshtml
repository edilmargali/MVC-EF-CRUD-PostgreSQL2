@model IEnumerable<MVC_EF_CRUD_PostgreSQL2.Models.EmpClass>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.empnombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.salario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.empnombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.salario)
        </td>
        <td>
            @Html.ActionLink("EDITAR", "Edit", new { id=item.empid }) |
            @Html.ActionLink("DETALLES", "Details", new { id=item.empid }) |
            @Html.ActionLink("ELIMINAR", "Delete", new { id=item.empid })
        </td>
    </tr>
}

</table>
